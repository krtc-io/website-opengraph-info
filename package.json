    {
      "path": "dist/website-opengraph-info.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/node-fetch": "2",
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "ts-node": "^10.8.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "node-abort-controller": "^3.0.1",
    "node-fetch": "2"
  }
}
